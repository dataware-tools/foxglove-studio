{
  "testMatch": [
    "<rootDir>/src/**/*.test.ts(x)?"
  ],
  "transform": {
    "\\.jsx?$": [ "babel-jest", { "rootMode": "upward" } ],
    "\\.tsx?$": "<rootDir>/../studio-base/src/test/transformers/typescriptTransformerWithRawImports.js",
    "\\.ne$": "<rootDir>/../studio-base/src/test/transformers/neTransformer.js",
    "\\.(bin|template|wasm)$": "<rootDir>/../studio-base/src/test/transformers/rawTransformer.js"
  },
  "transformIgnorePatterns": [ ],
  "globals": {
    "ReactNull": null,
    "FOXGLOVE_STUDIO_VERSION": "TEST"
  },
  "setupFiles": [ "<rootDir>/../studio-base/src/test/setup.ts", "jest-canvas-mock", "fake-indexeddb/auto" ],
  "setupFilesAfterEnv": [ "<rootDir>/../studio-base/src/test/setupTestFramework.ts" ],
  "restoreMocks": true,
  "moduleNameMapper": {
    "@foxglove/studio-base/(.*)": "<rootDir>/../studio-base/src/$1",
    "\\.svg$": "<rootDir>/../studio-base/src/test/mocks/MockSvg.tsx",
    "\\.css$": "<rootDir>/../studio-base/src/test/mocks/MockCss.ts",
    "react-monaco-editor": "<rootDir>/../studio-base/src/test/stubs/MonacoEditor.tsx",
    "\\.(glb|md|png)$": "<rootDir>/../studio-base/src/test/mocks/fileMock.ts"
  },
  "haste": { "forceNodeFilesystemAPI": true },
  "//": "above setting is copied from foxglove-studio/packages/studio-base/jest.config.json",
  "testEnvironment": "jsdom"
}
